public class Main {
    public static void main(String[] args) {
        int a = 10;
        int b = 20;
        int c = 30;
        int x = 40;

        //equazione ax^2 + bx + c =0
        int equazione = (int) (-b +-(Math.sqrt(((int) Math.pow(b, 2)) - 4 * a * c)) / (2*a));
        System.out.println(equazione);
    }
