public class Main {
    public static void main(String[] args) {
    int a = 10;
    int b = 20;
    int x = 30;
    int c = 40;

    int moltiplicazione1 = a * x;
    int potenza = (int) Math.pow(moltiplicazione1, 2);
    int moltiplicazione2 = b * x;
    int equazione = potenza + moltiplicazione2 + c;
        System.out.println(equazione);

    }
}
